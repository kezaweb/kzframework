{
    "require": {
        "propel/propel1": "1.*",
        "monolog/monolog": "1.0.*",
        "phpunit/phpunit": "3.7.*",
        "phpunit/DbUnit": "*",
        "smarty/smarty": "3.*",
        "symfony/http-foundation": "2.1.*",
        "symfony/routing": "2.1.*",
        "symfony/http-kernel": "2.1.*"
    },
    "autoload": {
        "psr-0": {
            "Kzf/Engine"            : "build/classes/engine/",
            "Kzf/Engine/Base"       : "build/classes/engine/base",
            "Kzf/Model"             : "build/classes/model/",
            "Kzf/Controller"        : "build/controllers/",
            "Kzf/Controller/Base"   : "build/controllers/base/"
        },
        "classmap": ["build/classes/", "build/controllers/"]
    }
}
